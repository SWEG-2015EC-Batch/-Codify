//this program prints elements of an array in 3 columns

#include <iostream>

using namespace std;

int main()
{
    int i;
   float volts[9]{11.95, 16.32, 12.15, 8.22, 15.98,
26.22, 13.54, 6.45, 17.59};
for(i=0;i<3;i++){
cout<<volts[i]<<"\t";
}
cout<<endl;
for(i=3;i<6;i++){
cout<<volts[i]<<"\t";
}
cout<<endl;
for(i=6;i<9;i++){
cout<<volts[i]<<"\t";
}
    return 0;
}
